{
  "name": "PlatePilot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.23.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/lab": "^5.0.0-alpha.153",
    "@mui/material": "^5.14.17",
    "@react-buddy/ide-toolbox": "^2.4.0",
    "@react-buddy/palette-mui": "^5.0.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react-helmet": "^6.1.11",
    "axios": "^1.6.1",
    "js-sha256": "^0.10.1",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.18.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "NODE_ENV=development && concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint . --fix ",
    "init_db": "cd ../db && python ./initialize.py",
    "test": "npm run init_db && concurrently \"npm run server\" \"BROWSER=none npm run client \" \"npx cypress run\"",
    "client": "react-scripts -r @cypress/instrument-cra start",
    "server": "node ../server/server.js",
    "build": "NODE_ENV=production PUBLIC_URL=/platepilot react-scripts build",
    "eject": "react-scripts eject",
    "prepare": "cd .. && husky install app/.husky",
    "precommit": "npx prettier . --write && npm run lint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.23.3",
    "@cypress/code-coverage": "^3.12.9",
    "@cypress/instrument-cra": "^1.4.0",
    "babel-plugin-istanbul": "^6.1.1",
    "concurrently": "^8.2.2",
    "cypress": "^13.5.1",
    "cypress-react-selector": "^3.0.0",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^15.1.0",
    "prettier": "3.0.3"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
